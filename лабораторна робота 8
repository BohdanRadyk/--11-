#include <stdio.h>
#include <math.h>

int main() {
  printf("Аргумент: -612367.54783\n");
  printf("fabs: %.5f\n", fabs(-612367.54783));
  return 0;
}





#include <stdio.h>
#include <math.h>

int main() {
    printf("Аргументи: 2.6 1 3.45\n");
    printf("pow: %.20f\n", pow(2.6, 1));
    printf("powl: %.20Lf\n", powl(2.6, 1));
    return 0;
}






#include <stdio.h>
#include <math.h>

int main()
{
    printf("Аргумент: 1.23\n");
    printf("cosl: %.20Lf\n", cosl(1.23));
    printf("cos: %.20f\n", cos (1.23));

    return 0;
}





#include <stdio.h>
#include <math.h>

int main() {
    double a, b, c;
    double D;
    double x_1, x_2;

    a = 1;
    b = 3;
    c = -4;

    D = b * b - 4 * a * c;

    if (D >= 0) {
        x_1 = (-b + sqrt(D)) / (2 * a);
        x_2 = (-b - sqrt(D)) / (2 * a);
        printf("Перший корінь: %lf\nДругий корінь: %lf\n", x_1, x_2);
    } else {
        printf("Дискримінант від'ємний, рівняння не має дійсних коренів.\n");
    }

    return 0;
}




#include <stdio.h>
#include <math.h>
int main() {
    double x, z;
    printf("Введіть значення x: ");
    scanf("%lf", &x);
    printf("Введіть значення z: ");
    scanf("%lf", &z);
    double result = (z + x + cos(x)) / (pow(sin(x) * cos(x), 2) + 16 * z);
    printf("Результат: %lf\n", result);
    return 0;
}




